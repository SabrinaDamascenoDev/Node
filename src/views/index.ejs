<!DOCTYPE html>
<html lang="en">
<%- include('./partils/header.ejs')%>
<body>
    <div class="container">
        <div class="content">
            <% if(users.length > 0) { %> <% users.forEach(user => { %>
                <h3 class="title">
                    <%= user.firstName %> <%= user.lastName %>
                </h3>
                <p><%= user.email %></p>

                <% }) %> <% } else { %> 
                    <p>Não há usuários para exibir.</p>
                <% } %> 
        </div>
    </div>
</body>
</html>